{
  "name": "meet-cpa",
  "shorName": "cpa",
  "version": "1.0.0",
  "main": "bin/index.js",
  "bin": "./dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node bin/index.js",
    "reform": "babel bin --out-dir dist --extensions '.js' --copy-files",
    "lint": "eslint bin --fix",
    "build:pkg": "npm run reform && pkg .",
    "build": "sh run.sh"
  },
  "lint-staged": {
    "bin/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.23.6",
    "aws-sdk": "^2.1470.0",
    "axios": "^1.5.1",
    "cli-progress": "^3.12.0",
    "crypto": "^1.0.1",
    "dotenv": "16.0.3",
    "fs": "^0.0.1-security",
    "i18n": "^0.15.1",
    "mongodb": "5.4",
    "mongoose": "^7.6.2",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "node-telegram-bot-api": "0.61.0",
    "path": "^0.12.7",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [
      "bin/locales/*.json",
      "node_modules/axios/dist/**/*"
    ],
    "outputPath": "./dist",
    "targets": [
      "node18-linux-x64"
    ]
  }
}
